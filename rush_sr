library(tidyverse)
library(nflreadr)
library(httr)
library(jsonlite)

options(scipen = 9999)

rush_sr <- load_pbp(2024) %>%
  filter(!is.na(epa), !is.na(down), rush == 1) %>%
  filter(posteam == "NYG") %>%
  group_by(week) %>%
  reframe(plays = n(),
          epa = mean(epa),
          sr = mean(ifelse(epa > 0, 1, )))
